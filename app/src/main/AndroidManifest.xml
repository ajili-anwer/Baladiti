        <activity android:name=".Annuaire" />
        <activity android:name=".Signup" />
        <activity android:name=".Doc_tech" />
        <activity android:name=".Login" />
        <activity android:name=".Demande_information" />
        <activity android:name=".Liste_demande" />
        <activity android:name=".Aj_information" />
        <activity android:name=".Annuaire_ret" />
        <activity android:name=".annuaire_nv" />
        <activity android:name=".Membre_muni" />
        <activity android:name=".Profil_detail" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Dashbord" />
        <activity android:name=".Map" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".FetchAdress" />
    </application>

</manifest>