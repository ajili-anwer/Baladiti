<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="190dp"
    android:background="#C0C0C0"
    app:cardCornerRadius="9dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="185dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="1dp">


        <TextView
            android:id="@+id/activite_tv"
            android:layout_width="289dp"
            android:layout_height="41dp"
            android:layout_margin="10dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="10dp"
            android:hint="activitÃ©"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/slug_tv1"
            android:layout_width="282dp"
            android:layout_height="27dp"
            android:layout_margin="10dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="10dp"
            android:hint="slug"
            android:textColor="	#696969"
            android:textSize="10sp"
            app:layout_constraintBottom_toTopOf="@+id/imageButton1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/adresse_tv" />

        <TextView
            android:id="@+id/adresse_tv"
            android:layout_width="288dp"
            android:layout_height="27dp"
            android:layout_margin="10dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="8dp"
            android:hint="adresse"
            android:textSize="10sp"
            app:layout_constraintBottom_toTopOf="@+id/slug_tv1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/activite_tv" />

        <ImageButton
            android:id="@+id/imageButton1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="80dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/btn_ann"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/phone"
            tools:ignore="VectorDrawableCompat" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="18dp"
            android:background="@drawable/btn_ann"
            app:layout_constraintEnd_toStartOf="@+id/imageButton1"
            app:layout_constraintStart_toEndOf="@+id/imageButton4"
            app:layout_constraintTop_toBottomOf="@+id/slug_tv1"
            app:srcCompat="@drawable/ic_fax"
            tools:ignore="MissingConstraints,VectorDrawableCompat" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="58dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/btn_ann"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/slug_tv1"
            app:srcCompat="@drawable/mail"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="107dp"
            android:layout_height="164dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="11dp"
            android:background="@drawable/r12"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="5dp"
            android:background="#E0E0E0"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>





